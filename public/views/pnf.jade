div.col-xs-12.col-md-12.vertical-center
    div.row
        h1 Player Not Found
        div.col-xs-12.col-md-12
            .input-group(ng-class='getSearchClass()')
                input#searchField.form-control(type='text', placeholder='Find Players', ng-model='gamertagInput', name='searchField', onkeydown="if (event.keyCode == 13) document.getElementById('searchButton').click()")
                span.input-group-btn
                    button#searchButton.btn.btn-default(ng-click='search(gamertagInput)')
                        i.fa.fa-search
